<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maui.app1.EmbeddingsPage"
             Title="EmbeddingsPage">
    <VerticalStackLayout>
        <HorizontalStackLayout VerticalOptions="End" HorizontalOptions="Center" IsVisible="False" x:Name="picker">
            <Button Text="Embed/process documents for AI" HorizontalOptions="Start" Clicked="Button_Clicked" VerticalOptions="End" IsEnabled="False" x:Name="btnProcess"/>
            <Picker x:Name="FriendsPicker" Title="Choose an option" IsEnabled="False"/>
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>